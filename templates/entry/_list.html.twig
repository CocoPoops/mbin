{% for entry in entries %}
    {{ component('entry', {entry: entry, showMagazineName: magazine is not defined or not magazine}) }}
{% endfor %}
{% if(entries.haveToPaginate is defined and entries.haveToPaginate) %}
    <div class="section">
        {{ pagerfanta(entries, null, {'pageParameter':'[p]'}) }}
    </div>
{% endif %}
{% if not entries|length %}
    <aside class="section section--muted user-list">
        <p>{{ 'empty'|trans }}</p>
    </aside>
{% endif %}
